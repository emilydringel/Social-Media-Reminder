{
  "name": "Social Media Reminder",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/", "https://twitter.com/"],
      "js": ["content.js"]
    }
  ],
  "browser_action":{
    "default_popup": "popup.html",
    "default_title": "Reminder"
  }
}
